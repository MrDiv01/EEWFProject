@using EEWF.Domain.DTOs;
@model List<CaruselDto>
<aside id="fh5co-hero">
    <div class="flexslider">
        <ul class="slides">
            @foreach (CaruselDto carusel in Model)
            {
                <li style="background-image: url(../assets/images/img_bg_1.jpg);">
                    <div class="overlay-gradient"></div>
                    <div class="container">
                        <div class="row">
                            <div class="col-md-8 col-md-offset-2 text-center slider-text">
                                <div class="slider-text-inner">
                                    <h1>@carusel.Header</h1>
                                    <h2>@carusel.Title</h2>
                                    <p><a class="btn btn-primary btn-lg" href="@carusel.Url">Show</a></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
            }
        </ul>
    </div>
</aside>

